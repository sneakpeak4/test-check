{
  "name": "delivery-slot-service-ui",
  "description": "Delivery Slot Service UI angular application",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ng serve -c local",
    "prepare": "cd ../ && husky install ./delivery-slot-service-ui/.husky",
    "build:local": "ng build -c local && yarn build:version-file",
    "build:dev": "ng build -c development && yarn build:version-file",
    "build:prod": "ng build && yarn build:version-file",
    "build:version-file": "sed \"s/<version>/$npm_package_version.${VERSION_LABEL:-local}/g\" version.json > dist/version.json",
    "test": "ng test",
    "test:ci": "ng test --no-watch --browsers=ChromeHeadlessCI",
    "format:check": "prettier --config ./.prettierrc --list-different \"./**/*{.html,.js,.ts,.json,.css,.scss}\"",
    "format:write": "prettier --config ./.prettierrc --write \"./**/*{.html,.ts,.js,.json,.css,.scss}\"",
    "format:staged": "pretty-quick --staged",
    "lint": "ng lint",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.ts": "eslint --config ./.eslintrc.json --fix"
  },
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/material": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@danielmoncada/angular-datetime-picker": "^14.0.1",
    "@js-joda/core": "^5.2.0",
    "@js-joda/timezone": "^2.12.0",
    "@picnic/api-handling": "^8.0.0",
    "@picnic/material": "^11.2.0",
    "@picnic/pipe": "^8.0.0",
    "@picnic/security": "^12.0.0",
    "@picnic/time-format": "^11.0.0",
    "@picnic/update-check": "^11.0.0",
    "@picnic/utils": "^12.0.0",
    "font-awesome": "^4.7.0",
    "lodash-es": "^4.17.21",
    "reset-css": "^5.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~5.1.0",
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "typescript": "~4.7.2"
  }
}
